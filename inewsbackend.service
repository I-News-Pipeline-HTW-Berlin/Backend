[Unit]
Description=I-News Backend
After=network.target

[Service]
Environment=JAVA_HOME=/opt/jdk-11
Type=simple
User=backend
WorkingDirectory=/home/backend
ExecStart=/opt/jdk-11/bin/java -jar /home/backend/inews-backend-assembly-0.1.jar
Restart=always
RestartSec=3


[Install]
WantedBy=multi-user.target
